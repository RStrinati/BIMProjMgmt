# Database Configuration
# Copy this file to .env and fill in actual values
DB_DRIVER=ODBC Driver 17 for SQL Server
DB_SERVER=your_server_here
DB_USER=your_username_here
DB_PASSWORD=your_secure_password_here

# Database Names
PROJECT_MGMT_DB=ProjectManagement
WAREHOUSE_DB=ProjectManagement
ACC_DB=acc_data_schema
REVIT_HEALTH_DB=RevitHealthCheckDB
REVIZTO_DB=ReviztoData

# External Service Endpoints
ACC_SERVICE_URL=http://localhost:4000/api/v1
REVIZTO_SERVICE_URL=http://localhost:5000/api/v1
APS_AUTH_SERVICE_URL=http://localhost:3000
APS_AUTH_LOGIN_PATH=/login-pkce

# Service Auth Tokens (managed by token broker)
ACC_SERVICE_TOKEN=
REVIZTO_SERVICE_TOKEN=

# Logging
LOG_LEVEL=INFO
LOG_JSON=true
