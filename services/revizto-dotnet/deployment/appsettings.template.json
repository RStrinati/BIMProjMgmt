{
  "ReviztoAPI": {
    "AccessToken": "REPLACE_WITH_YOUR_ACCESS_TOKEN",
    "RefreshToken": "REPLACE_WITH_YOUR_REFRESH_TOKEN",
    "BaseUrl": "https://app.revizto.com/api",
    "ApiKey": "REPLACE_WITH_YOUR_API_KEY"
  },
  "Database": {
    "ConnectionString": "Server=YOUR_SQL_SERVER;Database=ReviztoData;Trusted_Connection=True;"
  },
  "ExportSettings": {
    "OutputDirectory": "Exports",
    "LogDirectory": "logs"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}